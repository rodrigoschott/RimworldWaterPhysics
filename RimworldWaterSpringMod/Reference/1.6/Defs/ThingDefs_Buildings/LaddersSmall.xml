<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="MF_StairEntranceBase">
		<defName>MF_LadderSUpA</defName>
		<label>small ladder up</label>
		<description>A compact entrance leading to the upper level. Requires an indoor, roofed room. Cannot be built underground.</description>
		<rotatable>false</rotatable>
		<size>(1, 1)</size>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>25</costStuffCount>
		<statBases>
			<WorkToBuild>1000</WorkToBuild>
		</statBases>
		<uiOrder>3010</uiOrder>
		<uiIconScale>0.9</uiIconScale>
		<uiIconOffset>(0, 0.12)</uiIconOffset>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>MultiFloors/Buildings/LadderSmall/LadderSUp</texPath>
			<drawSize>(2, 2)</drawSize>
			<drawOffset>(0, 0, 0.55)</drawOffset>
		</graphicData>
		<modExtensions>
			<li Class="MultiFloors.StairsModExtension">
				<stairsSettings>
					<isUpstairs>true</isUpstairs>
					<connectedToExitDef>MF_LadderSExitA</connectedToExitDef>
				</stairsSettings>
			</li>
		</modExtensions>
	</ThingDef>
	<ThingDef ParentName="MF_StairEntranceBase">
		<defName>MF_LadderSDownA</defName>
		<label>small ladder down</label>
		<description>A compact entrance leading to the lower level. Cannot be built on upper levels.</description>
		<rotatable>false</rotatable>
		<size>(1, 1)</size>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>25</costStuffCount>
		<statBases>
			<WorkToBuild>1000</WorkToBuild>
		</statBases>
		<uiOrder>3010</uiOrder>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>MultiFloors/Buildings/LadderSmall/LadderSDown</texPath>
			<drawSize>(2, 2)</drawSize>
		</graphicData>
		<modExtensions>
			<li Class="MultiFloors.StairsModExtension">
				<stairsSettings>
					<isUpstairs>false</isUpstairs>
					<connectedToExitDef>MF_LadderSExitA</connectedToExitDef>
				</stairsSettings>
			</li>
		</modExtensions>
	</ThingDef>
	<ThingDef ParentName="MF_StairExitBase">
		<defName>MF_LadderSExitA</defName>
		<label>small ladder exit</label>
		<description>The endpoint of a small ladder.</description>
		<rotatable>false</rotatable>
		<size>(1, 1)</size>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>25</costStuffCount>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>MultiFloors/Buildings/LadderSmall/LadderSUp</texPath>
			<drawSize>(2, 2)</drawSize>
		</graphicData>
		<modExtensions>
			<li Class="MultiFloors.StairsModExtension">
				<stairsSettings>
					<upstairsEntranceDef>MF_LadderSUpA</upstairsEntranceDef>
					<downstairsEntranceDef>MF_LadderSDownA</downstairsEntranceDef>
				</stairsSettings>
			</li>
		</modExtensions>
	</ThingDef>
</Defs>